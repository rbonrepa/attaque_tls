{
  "name": "sitattoo",
  "version": "1.0.0",
  "description": "melipone tattoo site",
  "main": "index.js",
  "keywords": [
    "tattoo",
    "tatouage",
    "Pamiers"
  ],
  "author": "Bonrepaux r√©mi",
  "license": "MIT",
  "scripts": {
    "start": "yarn build && npm-run-all --parallel watch:*",
    "watch:eleventy": "eleventy --serve --output=_pre-dist",
    "watch:static": "copy-and-watch --skip-initial-copy --watch './{assets,images}/**' _pre-dist",
    "watch:parcel": "parcel '_pre-dist/**/*.html' --out-dir _dist --cache-dir _cache --no-source-maps",
    "clean": "rimraf ./_dist ./_pre-dist",
    "build": "yarn clean && yarn build:eleventy && yarn build:static && yarn build:parcel",
    "build:eleventy": "eleventy --output=_pre-dist",
    "build:static": "copy-and-watch './{assets,images}/**' _pre-dist",
    "build:parcel": "parcel build --no-minify '_pre-dist/**/*.html' --out-dir _dist --cache-dir _cache --no-source-maps",
    "test": "yarn test:styl && yarn test:ts && echo ':)'",
    "test:styl": "stylelint './assets/**/*.styl' --custom-syntax stylelint-plugin-stylus/custom-syntax --fix",
    "test:ts": "eslint './assets/**/*.ts' --fix",
    "preview": "yarn build && yarn serve --no-clipboard _dist"
  },
  "devDependencies": {
    "@11ty/eleventy": "^0.11.0",
    "@11ty/eleventy-navigation": "^0.1.6",
    "@typescript-eslint/eslint-plugin": "^4.7.0",
    "@typescript-eslint/parser": "^4.7.0",
    "copy-and-watch": "^0.1.4",
    "eslint": "^7.2.0",
    "eslint-config-standard": "^16.0.1",
    "eslint-plugin-import": "^2.21.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "moment": "^2.27.0",
    "normalize.css": "^8.0.1",
    "npm-run-all": "^4.1.5",
    "parcel-bundler": "^1.12.4",
    "rimraf": "^3.0.2",
    "serve": "^11.3.2",
    "stylelint": "^13.5.0",
    "stylelint-config-rational-order": "^0.1.2",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-order": "^4.0.0",
    "stylelint-plugin-stylus": "^0.9.0",
    "stylelint-rscss": "^0.4.0",
    "stylus": "^0.54.7",
    "typescript": "^4.0.5"
  },
  "dependencies": {
    "@quasibit/eleventy-plugin-sitemap": "^2.1.5",
    "eleventy-plugin-googlestaticmaps": "^1.0.2",
    "nunjucks": "^3.2.3"
  }
}
